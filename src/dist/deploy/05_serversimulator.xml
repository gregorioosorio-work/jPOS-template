<server class="org.jpos.q2.iso.QServer" logger="Q2" name="simulator_8100">
    <attr name="port" type="java.lang.Integer">8100</attr>
    <channel class="org.jpos.iso.channel.ASCIIChannel" logger="Q2" packager="org.jpos.iso.packager.GenericPackager">
        <property name="packager-config" value="cfg/iso87ascii.xml"/>
    </channel>
    <request-listener class="org.jpos.bsh.BSHRequestListener" logger="Q2">
        <property name="source" value="cfg/serversimulator.bsh" />
    </request-listener>
</server>